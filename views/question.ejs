<%- include('header') %>


<div class="choose-area pb_90">
  <div class="container mt-4">
<div class="row">
  <div class="col-12">
    <div class="headline">
      <h2>Unanswered Questions</h2>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-12">
    <%  questions.forEach(function(question){ %>
    <div class="alert alert-success" role="alert">
      <h4 class="alert-heading"><%= question.person %></h4>
      <hr>
      <p class="mb-0"><%= question.question %></p>
    </div>
    <% }) %>
  </div>
</div>
</div>
</div>

<%- include('footer') %>
